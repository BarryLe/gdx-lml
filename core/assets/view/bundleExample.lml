<table>
    <!-- <tr/> makes a table row. -->
    @thisIsABundleText<tr/>
    @custom@thisIsABundleText<tr/>
    #thisIsAPreference<tr/>
    #custom#thisIsAPreference<tr/>
    &thisInvokesAction<tr/>
    ${thisIsAnArgument}<tr/>
    ${thisArgumentWasNotPassedAndShouldPrintNull}<tr/>
    @thisContainsArguments|First|Second|${argumentFromParser}
    <!-- See preparePreferences, prepareCustomPreferences,
      -- bundle.properties and custom.properties for details.
      -->
</table>