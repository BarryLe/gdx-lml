<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module>
	<source path="com/github/czyzby" />

	<!-- Autumn data processed with reflection is included in GWT reflection pool: -->
	<extend-configuration-property name="gdx.reflect.include" value="com.github.czyzby.context" />
	<!-- These LibGDX classes are accessed using reflection and need to be manually included: -->
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.utils.ObjectSet" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.scenes.scene2d.ui.Label" />
</module>