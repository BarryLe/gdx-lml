	<!-- Core class has to be reflected for LML annotation processing: -->
	<extend-configuration-property name="gdx.reflect.include" value="com.github.czyzby.Core" />
	<extend-configuration-property name="gdx.reflect.include" value="com.github.czyzby.kiwi.util.gdx.AbstractApplicationListener" />
